"$schema" = 'https://starship.rs/config-schema.json'

palette = "{% if theme == 'dark' %}gruvbox_dark{% else %}iceberg_light{% endif %}"

add_newline = true

[line_break]
disabled = false

[username]
style_user = 'blue bold'
style_root = 'red bold'
format = '[$user]($style)'
disabled = false
show_always = true

[character]
success_symbol = '[➜](bold green) '
error_symbol = '[✗](bold red) '

